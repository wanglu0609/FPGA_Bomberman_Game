module Map (
	input Reset, Frame_Clk,
	input [9:0] Bomb_X, Bomb_Y,
	output [99:0] Wall_Map, Tree_Map
);
	assign Wall_Map[99:90] = 10'b0000000000;
	assign Wall_Map[89:80] = 10'b0101001010;
	assign Wall_Map[79:70] = 10'b0101001010;
	assign Wall_Map[69:60] = 10'b0100000010;
	assign Wall_Map[59:50] = 10'b0101001010;
	assign Wall_Map[49:40] = 10'b0000000000;
	assign Wall_Map[39:30] = 10'b0101001010;
	assign Wall_Map[29:20] = 10'b0100000010;
	assign Wall_Map[19:10] = 10'b0101001010;
	assign Wall_Map[09:00] = 10'b0000000000;
	
endmodule
